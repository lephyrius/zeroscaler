{
  "name": "@zeroscaler/zeroscaler-cdk",
  "description": "AWS CDK constructs for ZeroScaler.io",
  "website": "https://zeroscaler.io",
  "homepage": "https://zeroscaler.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lephyrius/zeroscaler.git"
  },
  "version": "0.1.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "aws",
    "cdk",
    "awscdk",
    "zeroscaler",
    "fargate",
    "lambda"
  ],
  "author": "Jonas Innala",
  "license": "MPL-2.0",
  "readme": "README.md",
  "files": [
    "lib",
    "bootstrap",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "prepare": "npm run build",
    "package": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5",
    "@types/node": "22.7",
    "aws-cdk-lib": "2.190",
    "constructs": "^10.0",
    "jest": "^29.7",
    "ts-jest": "^29.2",
    "typescript": "~5.8",
    "publib": "^0.2"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2",
    "constructs": "^10"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jsii": {
    "stability": "stable",
    "types": "lib/index.d.ts",
    "jsii": {
      "versionFormat": "short",
      "targets": {
        "java": {
          "package": "io.zeroscaler",
          "maven": {
            "groupId": "io.zeroscaler",
            "artifactId": "cdk"
          }
        },
        "python": {
          "distName": "zeroscaler",
          "module": "cdk"
        },
        "dotnet": {
          "namespace": "ZeroScaler",
          "packageId": "ZeroScaler",
          "iconUrl": "https://zeroscaler.io/favicon.svg"
        },
        "go": {
          "packageName": "zeroscaler",
          "moduleName": "lephyrius/zeroscaler"
        }
      },
      "tsc": {
        "outDir": "dist"
      }
    }
  }
}
